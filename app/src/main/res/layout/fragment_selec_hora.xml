<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".aplicacion.ui.home.cincoSelecHora.SelecHora"
                                                   android:padding="15dp">

    <!-- TODO: Update blank fragment layout -->
    <TextView
            android:text="Horas disponibles"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/textView5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:gravity="center" android:padding="15dp"/>
    <Button
            android:text="Back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/backHoras"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="15dp"/>
    <ListView
            android:layout_width="0dp"
            android:layout_height="0dp" android:layout_marginTop="25dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5" app:layout_constraintBottom_toTopOf="@+id/backHoras"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/horasLista" android:layout_marginBottom="25dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>